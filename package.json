{
  "name": "mendix-hybrid-app-base",
  "version": "1.3.0",
  "description": "Mendix PhoneGap Build base package",
  "scripts": {
    "appbase": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.appbase.js",
    "pgpackage": "node ./node_modules/webpack/bin/webpack",
    "pgwatch": "node ./node_modules/webpack/bin/webpack --watch",
    "pgplatform": "cd build && phonegap platforms",
    "pgplugin": "cd build && phonegap plugin",
    "pgprepare": "cd build && phonegap prepare",
    "pgcompile": "cd build && phonegap compile",
    "pgbuild": "cd build && phonegap build",
    "pgrun": "cd build && phonegap run",
    "pgremote": "cd build && phonegap remote run"
  },
  "main": "src/www/scripts/entry.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "config",
    "src",
    "*.js",
    "yarn.lock",
    "README.md"
  ],
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "i18n-webpack-plugin": "^0.3.0",
    "mustache": "^2.1.2",
    "mustache-loader": "^0.4.1",
    "sanitize-filename": "^1.6.1",
    "style-loader": "^0.13.2",
    "tiny-emitter": "^1.1.0",
    "webpack": "^3.0.0",
    "webpack-archive-plugin": "^3.0.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git@github.com:mendix/hybrid-app-base.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Kevin Vlaanderen",
    "email": "kevin.vlaanderen@mendix.com",
    "url": "https://developer.mendixcloud.com/link/profile/15218"
  }
}
